    <!-- Options screen / Changelog -->
    <string name="options_changelog_app_updated">Aplicația a fost actualizată</string>
    <string name="options_changelog_what_is_new">Ce e nou</string>
    <string name="options_changelog_1_0_0_description">
        <Data><![CDATA[
        • Îmbunătățiri de <strong>animații</strong> și <strong>tranziții</strong><br>
        • <strong>Corecții de erori</strong> și <strong>îmbunătățiri de stabilitate</strong>
        ]]></Data>
    </string>
    <string name="options_changelog_0_9_0_description">
        <Data><![CDATA[
        • Opțiune pentru <strong>distribuirea</strong> pieselor și a listelor de piese<br>
        • Îmbunătățiri semnificative de <strong>design</strong> și <strong>performanță</strong><br>
        • Optimizări pentru <strong>ecrane mari</strong>
        ]]></Data>
    </string>
    <string name="options_changelog_0_8_0_description">
        <Data><![CDATA[
        • Buton de <strong>căutare</strong> pe paginile de Start și Colecții<br>
        • <strong>Algoritm de căutare</strong> îmbunătățit<br>
        • Opțiunea de <strong>a cumpăra o bere</strong> dezvoltatorului
        ]]></Data>
    </string>
    <string name="options_changelog_0_7_0_description">
        <Data><![CDATA[
        • <strong>Pagină de start</strong> care conține sugestii de piese și colecții<br>
        • <strong>Derulare rapidă</strong> pe pagina de Piese
        ]]></Data>
    </string>
    <string name="options_changelog_0_6_0_description">
        <Data><![CDATA[
        • <strong>Introducere în aplicație</strong> unde utilizatorii pot personaliza experiența<br>
        • Traducerea în <strong>română</strong><br>
        • Îmbunătățiri semnificative de <strong>design</strong>
        ]]></Data>
    </string>
    <string name="options_changelog_0_5_0_description">
        <Data><![CDATA[
        • <strong>Setări de limbă</strong> și traducere <strong>maghiară</strong><br>
        • Opțiune de schimbare automată între temă <strong>întunecată</strong> și <strong>luminată</strong> în funcție de perioada zilei<br>
        • <strong>Statistici de utilizare</strong> și <strong>raporturi de eroare</strong> opționale
        ]]></Data>
    </string>
    <string name="options_changelog_0_4_0_description">
        <Data><![CDATA[
        • <strong>Colecții</strong> tematice de piese<br>
        • Opțiunea de a regla <strong>mărimea fontului</strong><br>
        • Buton de <strong>amestecare</strong> care deschide piesele din lista curentă în ordine aleatoare
        ]]></Data>
    </string>
    <string name="options_changelog_0_3_0_description">
        <Data><![CDATA[
        • Posibilitatea de a crea <strong>liste</strong> locale<br>
        • Versuri <strong>paginate</strong> pentru piese din liste<br>
        • Suport pentru <strong>comenzi rapide</strong> pentru sistemele Android 7.1 sau mai recente
        ]]></Data>
    </string>
    <string name="options_changelog_0_2_0_description">
        <Data><![CDATA[
        • <strong>Transpunerea individuală</strong> a pieselor și salvarea setării<br>
        • <strong>Derulare automată</strong> cu viteză reglabilă<br>
        • <strong>Istoric</strong> unde se afișează lista pieselor deschise în ordine cronologică
        ]]></Data>
    </string>
    <string name="options_changelog_0_1_0_description">
        <Data><![CDATA[
        • <strong>Listă de piese</strong> cu opțiuni de căutare, sortare și filtrare<br>
        • <strong>Versuri</strong> cu sau fără acorduri<br>
        • <strong>Mod fără conexiune</strong> cu opțiunea de a șterge individual piesele salvate
        ]]></Data>
    </string>

